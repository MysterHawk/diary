{% extends 'base.html.twig' %}

{% block title %}
	Contact page
{% endblock %}

{% block description %}
	Page that contains contact information
{% endblock %}

{% block header %}

	🗣️ Contact

{% endblock %}


{% block main %}

	<ul>
		<li>
			<b>Name:</b>
			{{contact.name}}
			{{contact.surname}}
		</li>
		<li>
			<b>Email</b>:
			{{contact.email}}
		</li>
		<li>
			<b>Phone</b>:
			{{contact.phone}}
		</li>

	</ul>
	<br>
{% endblock %}
